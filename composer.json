{
	"name": "pulsar/pulsar",
	"description": "ORM implementing the Active Record pattern in PHP",
	"type": "library",
	"keywords": ["pulsar","database","orm","activerecord"],
	"license": "MIT",
	"authors": [
		{
			"name": "jaredtking",
			"email": "j@jaredtking.com",
			"homepage": "http://jaredtking.com"
		}
	],
	"support": {
		"issues": "https://github.com/jaredtking/pulsar/issues",
		"source": "https://github.com/jaredtking/pulsar"
	},
    "autoload": {
        "psr-4": {"Pulsar\\": "src/"}
    },
	"autoload-dev": {
		"psr-4": { "Pulsar\\Tests\\": "tests/" }
	},
	"require": {
		"php": ">=7.1",
		"icanboogie/inflector": "^1.3",
		"symfony/event-dispatcher": "^4.3",
		"jaqb/jaqb": "~1.4",
		"infuse/libs": "~1.0",
		"psr/cache": "~1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "6.0.*",
		"phpunit/php-code-coverage": "5.*",
		"satooshi/php-coveralls": "1.0.0",
		"mockery/mockery": "~1.0",
		"symfony/cache": "~3.1|~4.0",
		"infuse/infuse": "~1.4",
		"doctrine/dbal": "~2.5"
	}
}
