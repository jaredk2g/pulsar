	},
	"require": {
		"php": ">=8.0",
		"icanboogie/inflector": "^2.0",
		"symfony/event-dispatcher": ">=4.3",
		"jaqb/jaqb": "^1.6",
		"psr/cache": "~1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5",
		"phpunit/php-code-coverage": "^7.0",
		"satooshi/php-coveralls": "1.0.0",
		"mockery/mockery": "~1.0",
		"symfony/cache": "~3.1|~4.0",
		"doctrine/dbal": "~2.5",
		"defuse/php-encryption": "^2.2"
	}
}
